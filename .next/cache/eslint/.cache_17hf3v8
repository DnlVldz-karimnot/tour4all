\n                                  gap={5}\n                                  flexWrap={\"wrap\"}\n                                >\n                                  {optionsSelected.color.selected.length >\n                                    0 && (\n                                      <Typography sx={{ marginTop: \"10px\" }}>\n                                        Colores:\n                                      </Typography>\n                                    )}\n\n                                  {optionsSelected.color.selected.length > 0 &&\n                                    optionsSelected.color.selected.map(\n                                      (color: any, i: number) => {\n                                        return (\n                                          <Chip\n                                            key={i}\n                                            label={color}\n                                            color=\"secondary\"\n                                          />\n                                        );\n                                      }\n                                    )}\n\n                                  {optionsSelected.size.selected.length > 0 && (\n                                    <Typography sx={{ marginTop: \"10px\" }}>\n                                      Tamaos:\n                                    </Typography>\n                                  )}\n                                  {optionsSelected.size.selected.length > 0 &&\n                                    optionsSelected.size.selected.map(\n                                      (color: string, i: number) => {\n                                        return (\n                                          <Chip\n                                            key={i}\n                                            label={color}\n                                            color=\"primary\"\n                                          />\n        